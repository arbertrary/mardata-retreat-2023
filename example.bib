  groups    = {survey/overview paper},
  interhash = {fbfd36fdd05d7e1d78d6e0568a4be4ef},
  intrahash = {b02d9170a462eae3a373c66acd170aef},
  keywords  = {datalineage literaturereview provenance survey},
