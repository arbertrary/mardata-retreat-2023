https://github.com/visdesignlab/trrack

https://github.com/visdesignlab/trrack-vis},
  description = {Trrack: A Library for Provenance-Tracking in Web-Based Visualizations | IEEE Conference Publication | IEEE Xplore},
  doi         = {10.1109/VIS47514.2020.00030},
  file        = {:cutler2020trrack.pdf:PDF},
  interhash   = {76949e284886d8a6caed3d07a721f115},
  intrahash   = {796274520a78fc545a3780dc178621ab},
  keywords    = {library provenance},
  timestamp   = {2022-02-18T16:07:40.000+0100}
  }
  
@InProceedings{denies2013git2prov,
